{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl":"./",
    "paths":{
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
    },
  },
  "plugins": [
    {
      "name": "typescript-styled-plugin",
      "tags": [
        "styled",
        "css", /*and others that you like it!*/
      ]
    }
  ],
}
